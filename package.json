{
  "private": false,
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "name": "vue-alphanav",
  "version": "1.2.0",
  "description": "Alphabetical Navigation bar displayed using flexbox.",
  "main": "dist/vue-alphanav.umd.js",
  "unpkg": "dist/vue-alphanav.umd.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xavsio4/vue-alphanav.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "postpublish": "npm publish --ignore-scripts --@xavsio4:registry='http://registry.npmjs.org'",
    "build-bundle": "vue-cli-service build --target lib --name vue-alphanav ./src/components/Alphanav.vue",
    "build-compo": "vue-cli-service build --target wc --name vue-alphanav ./src/components/Alphanav.vue"
  },
  "keywords": [
    "vuejs",
    "navigation",
    "alphabetical"
  ],
  "author": "x. Villamuera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xavsio4/vue-alphanav/issues"
  },
  "homepage": "https://github.com/xavsio4/vue-alphanav#readme",
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
